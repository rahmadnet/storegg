<table id="example2" class="table table-bordered table-hover">
    <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Quantity</th>
      <th scope="col">Name</th>
      <th scope="col">Price</th>
      <th scope="col">Action</th>
    </tr>
    </thead>
    <tbody>
        <% let totalSemua = 0; %>
      <% nominal.forEach((nominal, i) => { i++ %>
        <% totalSemua += laporan.nominal    %>
    <tr>
        <th scope="row"><%= i %> </th>
        <th><%= nominal.coinQuantity %></th>
        <th><%= nominal.coinName %></th>
        <th><%= nominal.price %></th>
        <th>
            <a href="/nominal/edit/<%= nominal.id %> " class="badge mb-3 btn btn-warning">
                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24"
                fill="none" stroke="#000000" stroke-width="2" stroke-linecap="round"
                stroke-linejoin="round" class="feather feather-edit">
                <path d="M11 4H4a2 2 0 0 0-2 2v14a2 2 0 0 0 2 2h14a2 2 0 0 0 2-2v-7"></path>
                <path d="M18.5 2.5a2.121 2.121 0 0 1 3 3L12 15l-4 1 1-4 9.5-9.5z"></path>
            </svg> Edit</a>
            <a href="/nominal/delete/<%= nominal.id %>" class="badge mb-3 btn btn-danger" onclick="return confirm('yakin?');">
                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24"
                fill="none" stroke="#000000" stroke-width="2" stroke-linecap="round"
                stroke-linejoin="round" class="feather feather-trash-2">
                <polyline points="3 6 5 6 21 6"></polyline>
                <path
                    d="M19 6v14a2 2 0 0 1-2 2H7a2 2 0 0 1-2-2V6m3 0V4a2 2 0 0 1 2-2h4a2 2 0 0 1 2 2v2">
                </path>
                <line x1="10" y1="11" x2="10" y2="17"></line>
                <line x1="14" y1="11" x2="14" y2="17"></line>
            </svg> Hapus</a>
        </th>
    </tr>
    <% }) %>
    </tbody>
    <tfoot>
        <tr>
            <th></th>
            <th></th>
            <th></th>
            <th>Total:</th>
            <th><%=toRupiah(totalSemua)%></th>
        </tr>
    </tfoot>
  </table>

  <%
    function formatDate(tanggal){
    var bulan = new Array("Jan", "Feb", "Mar",  "Apr", "Mei", "Juni", "Juli", "Agust", "Sept",  "Okt", "Nov", "Des");
    var d       = new Date(tanggal);
    var day     = d.getDay();
    var date    = d.getDate();
    var month   = d.getMonth();
    var year    = d.getFullYear();
    return `${date} - ${bulan[month]} - ${year}`;
}
    function toRupiah(bilangan){
    var	reverse     = bilangan.toString().split('').reverse().join(''),
    ribuan 	    = reverse.match(/\d{1,3}/g);
    ribuan	    = ribuan.join('.').split('').reverse().join('');
    return 'Rp. '+ribuan
}
%>